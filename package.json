    "claude",
    "claude-code",
    "ai",
    "bootstrap",
    "project-setup",
    "developer-tools",
    "llm",
    "codegen",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/ischca/proteus",
  "repository": {
    "type": "git",
    "url": "https://github.com/ischca/proteus.git"
  },
  "bugs": {
    "url": "https://github.com/ischca/proteus/issues"
  },
  "type": "module",
  "bin": {
